{
    "name": "underdogg-forks/freelancer-sdk-php",
    "description": "Official Freelancer.com PHP SDK",
    "type": "library",
    "license": "LGPL-3.0-or-later",
    "keywords": ["freelancer", "api", "sdk"],
    "authors": [
        {
            "name": "Freelancer.com",
            "email": "api-support@freelancer.com"
        }
    ],
    "require": {
        "php": "^8.4",
        "guzzlehttp/guzzle": "^7.4.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "FreelancerSdk\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FreelancerSdk\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
